<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.5.1//EN"
  "http://google-web-toolkit.googlecode.com/svn/tags/2.5.1/distro-source/core/src/gwt-module.dtd">
<module rename-to='Rapla'>
  <inherits name='com.google.gwt.user.User'/>
  <inherits name='com.google.gwt.logging.Logging'/>
  <inherits name='com.google.gwt.xml.XML' />
  <inherits name='com.google.gwt.http.HTTP'/>
  <inherits name='com.google.gwt.i18n.CldrLocales' />
  <inherits name='com.google.gwt.i18n.I18N' />
  <inherits name='com.google.gwt.user.theme.clean.Clean'/>
  <inherits name='com.google.gwt.inject.Inject'/>
  <inherits name='org.rapla.Rapla_main_module'/>
  <inherits name='org.gwtbootstrap3.GwtBootstrap3Theme'/>
  <inherits name='org.gwtbootstrap3.extras.datetimepicker.DateTimePicker'/>
  <inherits name='org.gwtbootstrap3.extras.datepicker.DatePicker'/>
  <source path='client' />
  <source path='plugin'>
    <exclude name='**/server/**' />
  </source>
  <entry-point class='org.rapla.client.gwt.Rapla'/>
  <set-property name='user.agent' value='safari' />
  <extend-property name='locale' values='de' />
  <set-property name='locale' value='de' />
  <set-property name='gwt.logging.logLevel' value='INFO'/>
  <set-property name='gwt.logging.enabled' value='TRUE'/>
  <set-configuration-property name='devModeRedirectEnabled' value='true'/>
  <define-configuration-property name='extra.ginModules' is-multi-valued='true' />
  <extend-configuration-property name='extra.ginModules' value='org.rapla.client.plugin.weekview.gwt.WeekViewPlugin' />
  <extend-configuration-property name='extra.ginModules' value='org.rapla.client.plugin.tableview.gwt.ListViewPlugin' />
  <extend-configuration-property name='extra.ginModules' value='org.rapla.client.edit.reservation.sample.gwt.ReservationEditPlugin' />

</module>